<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.shivam.metroproject.fragment.TwoFragment">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/loginss"
        android:src="@drawable/loginss"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/relativee">



            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imagebelow">

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/imagebelow"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </RelativeLayout>


                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/maincontentt"
                    android:layout_width="match_parent"
                    android:layout_height="140dp"
                    android:layout_marginTop="10dp"
                    app:lottie_autoPlay="true"
                    app:lottie_loop="true"
                    app:lottie_rawRes="@raw/lottie" />

                <LinearLayout
                    android:id="@+id/linearr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="10dp"
                    android:gravity="clip_horizontal"
                    app:layout_constraintTop_toBottomOf="@id/forgetpass"

                    >

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:fontFamily="@font/archivo_black"
                            android:gravity="left"
                            android:text="Plan Your Journey"
                            android:textColor="@color/black"
                            android:textSize="16dp"
                            android:textStyle="normal"
                            android:verticalScrollbarPosition="left"
                            app:layout_constraintTop_toBottomOf="parent" />


                    </RelativeLayout>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/linearrr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10dp"
                    android:background="#075e54"
                    android:orientation="vertical"
                    app:layout_constraintTop_toBottomOf="@id/linearrr">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/imagebutt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="17dp"
                            android:backgroundTint="@color/white"
                            android:drawableBottom="@drawable/dropdown" />

                        <Spinner
                            android:id="@+id/spinner1"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginRight="30dp"
                            android:layout_toRightOf="@id/imagebutt"
                            android:background="@android:drawable/btn_dropdown"
                            android:backgroundTintMode="screen"
                            android:gravity="center"
                            android:paddingLeft="20dp"
                            android:paddingTop="10dp"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:textSize="10sp"
                            android:textStyle="bold" />


                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/imagebuttt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="15dp"
                            android:backgroundTint="@color/red"
                            android:drawableBottom="@drawable/dropdown" />


                        <Spinner
                            android:id="@+id/spinner2"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_marginTop="7dp"
                            android:layout_marginRight="30dp"
                            android:layout_toRightOf="@id/imagebuttt"
                            android:background="@android:drawable/btn_dropdown"
                            android:backgroundTintMode="screen"
                            android:gravity="center"
                            android:paddingLeft="20dp"
                            android:paddingTop="10dp"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:textSize="10sp"
                            android:textStyle="bold" />

                    </RelativeLayout>


                    <LinearLayout
                        android:id="@+id/linerr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="20dp"
                        android:gravity="clip_horizontal"
                        app:layout_constraintTop_toBottomOf="@id/forgetpass">


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">


                            <EditText
                                android:id="@+id/datepicker"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginRight="30dp"
                                android:autofillHints="password"
                                android:background="@drawable/edittext_tb"
                                android:backgroundTint="@color/white"
                                android:clickable="false"
                                android:cursorVisible="false"
                                android:focusable="false"
                                android:focusableInTouchMode="false"
                                android:hint="Select Date"
                                android:paddingLeft="20dp"
                                android:paddingTop="10dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="10dp"
                                android:textColor="@color/black"
                                android:textColorHint="@color/lighlack"
                                android:textSize="12sp"
                                android:visibility="visible" />


                            <EditText
                                android:id="@+id/timepicker"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginRight="30dp"
                                android:autofillHints="password"
                                android:background="@drawable/edittext_tb"
                                android:backgroundTint="@color/white"
                                android:clickable="false"
                                android:cursorVisible="false"
                                android:focusable="false"
                                android:focusableInTouchMode="false"
                                android:hint="Select Time"
                                android:paddingLeft="20dp"
                                android:paddingTop="10dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="10dp"
                                android:textColor="@color/black"
                                android:textColorHint="@color/lighlack"
                                android:textSize="12sp"
                                android:visibility="visible" />


                        </RelativeLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/edittext_tb"
                        android:backgroundTint="@color/white"
                        android:gravity="clip_horizontal"
                        app:layout_constraintTop_toBottomOf="@id/forgetpass">


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="8dp"
                                android:layout_marginBottom="20dp"
                                android:paddingStart="8dp"
                                android:paddingEnd="8dp"
                                android:paddingBottom="20dp"
                                android:text="Advance Filters"
                                android:textColor="@color/lighlack"
                                android:textSize="10dp" />


                            <RadioButton
                                android:id="@+id/shortestroute"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentBottom="true"
                                android:autofillHints="password"
                                android:clickable="true"
                                android:cursorVisible="true"
                                android:focusable="true"
                                android:focusableInTouchMode="true"
                                android:hint="Shortest \n Route"
                                android:paddingLeft="5dp"
                                android:paddingTop="10dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="10dp"
                                android:textColor="@color/black"
                                android:textColorHint="@color/lighlack"
                                android:textSize="10sp"
                                android:textStyle="bold"
                                android:visibility="visible" />


                            <RadioButton
                                android:id="@+id/Advancefilter"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentBottom="true"
                                android:autofillHints="password"
                                android:clickable="true"
                                android:cursorVisible="true"
                                android:focusable="true"
                                android:focusableInTouchMode="true"
                                android:hint="Minimum \n Interchange"
                                android:paddingLeft="5dp"
                                android:paddingTop="10dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="10dp"
                                android:textColor="@color/black"
                                android:textColorHint="@color/lighlack"
                                android:textSize="10sp"
                                android:textStyle="bold"
                                android:visibility="visible" />


                        </RelativeLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/fairshow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">


                            <Button
                                android:id="@+id/showfair"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="15dp"
                                android:backgroundTint="@color/purple_700"
                                android:fontFamily="@font/archivo_black"
                                android:foregroundGravity="center_horizontal"
                                android:gravity="center"
                                android:paddingStart="10dp"
                                android:paddingRight="10dp"
                                android:text="@string/show_route_fare"
                                android:textColor="@color/white"
                                android:textSize="10dp" />


                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center|center_vertical"
                        android:orientation="horizontal">


                        <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_margin="10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textresult"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:text="Fare :"
                                android:textSize="30dp" />


                            <TextView
                                android:id="@+id/BkCostFld"
                                android:layout_width="0dp"
                                android:layout_height="35dp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginStart="40dp"
                                android:layout_marginEnd="60dp"
                                android:focusable="false"
                                android:gravity="center"
                                android:inputType="numberDecimal"
                                android:textColor="@color/white"
                                android:textSize="30dp"
                                android:textStyle="bold" />


                        </TableRow>



                    </LinearLayout>

                    <com.google.android.gms.ads.AdView
                        xmlns:ads="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/adVieww"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center"
                        android:layout_alignParentBottom="true"
                        ads:adSize="BANNER"
                        ads:adUnitId="ca-app-pub-3647835717520066/3591325640">
                    </com.google.android.gms.ads.AdView>




                </LinearLayout>



                <!-- set Banner ad position in UI layout design -->



                <LinearLayout
                    android:id="@+id/progressbar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/showroute"
                    android:gravity="center_vertical"
                    android:orientation="vertical">


                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/imageone"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:layout_alignParentLeft="true"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/imageback"
                            android:src="@drawable/traineeee" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/imageone"
                            android:layout_alignParentStart="true"
                            android:layout_centerInParent="true"
                            android:layout_marginStart="60dp"
                            android:layout_toEndOf="@id/imageone"
                            android:gravity="center"
                            android:text="Search Station"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold" />


                        <ImageButton
                            android:id="@+id/imagetwo"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="30dp"
                            android:background="@drawable/imageback"
                            android:paddingStart="20dp"
                            android:paddingTop="20dp"
                            android:paddingEnd="20dp"
                            android:paddingBottom="22dp"
                            android:src="@drawable/trainnnn" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/imagetwo"
                            android:layout_alignParentEnd="true"

                            android:layout_marginEnd="30dp"
                            android:layout_toEndOf="@id/imageone"
                            android:gravity="end"
                            android:text="Nearest Metro Station"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/imagethree"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="30dp"
                            android:background="@drawable/imageback"
                            android:paddingStart="20dp"
                            android:paddingTop="20dp"
                            android:paddingEnd="20dp"
                            android:paddingBottom="22dp"
                            android:src="@drawable/recharg" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/imagethree"
                            android:layout_alignParentEnd="true"
                            android:layout_centerInParent="true"
                            android:layout_marginEnd="50dp"

                            android:layout_toEndOf="@id/imagefour"
                            android:gravity="end"
                            android:text="Online Recharge"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold" />

                        <ImageButton
                            android:id="@+id/imagefour"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:layout_alignParentLeft="true"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/imageback"
                            android:src="@drawable/clock" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/imagefour"
                            android:layout_alignParentStart="true"
                            android:layout_centerInParent="true"
                            android:layout_marginStart="40dp"
                            android:layout_toEndOf="@id/imagefour"
                            android:gravity="center"
                            android:text="@string/first_last_metro_time"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="30dp"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/imageback"
                        android:backgroundTint="@color/grey">

                        <ImageView
                            android:id="@+id/stationalert"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_alignParentStart="true"
                            android:layout_marginStart="30dp"
                            android:src="@drawable/stationalertt" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/stationalert"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="110dp"
                            android:layout_marginTop="20dp"
                            android:text="Station Alert"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            android:textStyle="bold" />


                        <Switch
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="30dp" />

                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/linerrr"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="30dp"
                        android:layout_marginBottom="30dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">


                            <com.google.android.material.floatingactionbutton.FloatingActionButton
                                android:id="@+id/farecal"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="10dp"
                                android:backgroundTint="@color/grey"
                                android:clickable="true"
                                android:src="?android:attr/actionModeFindDrawable"
                                android:visibility="visible"
                                app:borderWidth="1dp"
                                app:fabSize="mini" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="50dp"
                                android:text="Fare Calculator"
                                android:textColor="@color/black"
                                android:textSize="11sp"
                                android:textStyle="bold" />

                            <com.google.android.material.floatingactionbutton.FloatingActionButton
                                android:id="@+id/farecall"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginEnd="30dp"
                                android:backgroundTint="@color/grey"
                                android:clickable="true"
                                android:src="@android:drawable/ic_menu_myplaces"
                                android:visibility="visible"
                                app:borderWidth="1dp"
                                app:fabSize="mini" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="50dp"
                                android:layout_marginEnd="30dp"
                                android:text="Tour Guide"
                                android:textColor="@color/black"
                                android:textSize="11sp"
                                android:textStyle="bold" />

                            <com.google.android.material.floatingactionbutton.FloatingActionButton
                                android:id="@+id/farecalll"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="10dp"
                                android:layout_marginTop="100dp"
                                android:backgroundTint="@color/grey"
                                android:clickable="true"
                                android:src="@android:drawable/ic_dialog_info"
                                android:visibility="visible"
                                app:borderWidth="1dp"
                                app:fabSize="mini" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="8dp"
                                android:layout_marginTop="150dp"
                                android:text="Other Info"
                                android:textColor="@color/black"
                                android:textSize="11sp"
                                android:textStyle="bold" />


                            <com.google.android.material.floatingactionbutton.FloatingActionButton
                                android:id="@+id/farecallll"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="100dp"
                                android:layout_marginEnd="30dp"
                                android:backgroundTint="@color/grey"
                                android:clickable="true"
                                android:src="@android:drawable/ic_menu_today"
                                android:visibility="visible"
                                app:borderWidth="1dp"
                                app:fabSize="mini" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="150dp"
                                android:layout_marginEnd="30dp"
                                android:text="@string/lost_found"
                                android:textColor="@color/black"
                                android:textSize="11sp"
                                android:textStyle="bold" />

                        </RelativeLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/backbox"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="10dp"
                            android:text="Important Information"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:textStyle="bold" />

                        <com.google.android.gms.ads.AdView
                            xmlns:ads="http://schemas.android.com/apk/res-auto"
                            android:id="@+id/adView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:layout_alignParentBottom="true"
                            ads:adSize="BANNER"
                            ads:adUnitId="ca-app-pub-3647835717520066/4433348615">
                        </com.google.android.gms.ads.AdView>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:layout_marginTop="30dp"
                            android:fontFamily="@font/archivo_black"
                            android:text="Quick Contact:"
                            android:textColor="@color/red"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@color/white">

                            <com.google.android.material.floatingactionbutton.FloatingActionButton
                                android:id="@+id/helpline"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="10dp"
                                android:layout_marginTop="5dp"
                                android:backgroundTint="@color/grey"
                                android:clickable="true"
                                android:src="@android:drawable/ic_menu_call"
                                android:visibility="visible"
                                app:borderWidth="1dp"
                                app:fabSize="mini" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_marginStart="66dp"
                                android:layout_marginTop="14dp"
                                android:layout_toEndOf="@id/helpline"
                                android:gravity="center_horizontal"
                                android:text="KMRC Helpline No."
                                android:textColor="@color/black"
                                android:textSize="12dp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="wrap_content"
